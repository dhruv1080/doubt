#include <iostream>
using namespace std;

int main()
{
    cout << "enter a no. = ";
    int num;
    cin >> num;
    bool divided = false;
    for (int i=2;i<num;i++)
    {
        if (num % i == 0)
        {
            cout << "not prime" << endl;
            divided = true;
            break;
        }
        i++;
    }
    if (!divided)
    {
        cout << "prime" << endl;
    }
    
}
